{
  "name": "@activadee/n8n-nodes-gradio-client",
  "version": "0.0.1",
  "description": "n8n node for connecting to Gradio Spaces",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "gradio",
    "huggingface",
    "ai"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activadee/n8n-nodes-gradio-client.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "cp -r nodes/GradioClient/*.svg dist/nodes/GradioClient/",
    "dev": "tsc --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HuggingFaceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GradioClient/GradioClient.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.82.0"
  }
}
